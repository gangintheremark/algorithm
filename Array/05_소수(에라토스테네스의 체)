import java.util.*;

class Main {
        /*
         1. ch[i] ê°€ 0 ì´ë©´ ì†Œìˆ˜, 1ì´ë©´ ì†Œìˆ˜X
         2. ì‹œê°„ì„ ì¤„ì´ê¸° ìœ„í•´ iì˜ ë°°ìˆ˜ë“¤ì€ 1ë¡œ ì²´í¬
         3. 1ë¡œ ì²´í¬ëœ iëŠ” íŒ¨ìŠ¤
         ğŸ‘‰ ì—ë¼í† ìŠ¤í…Œë„¤ìŠ¤ ì²´
         */
    public int solution(int n) {
        int answer = 0;
        int[] ch = new int[n+1];
        for (int i = 2; i <= n; i++) {
            if(ch[i] == 0) {
                answer++;
                for (int j = i; j <=n ; j+=i) { //iì˜ ë°°ìˆ˜ë‹ˆê¹ iì”© ì¦ê°€
                    ch[j]++;
                }
            }
        }
        return answer;
    }

    public static void main(String[] args) {
        Main T = new Main();
        Scanner s = new Scanner(System.in);

        int n = s.nextInt();

        System.out.println(T.solution(n));

    }

}

